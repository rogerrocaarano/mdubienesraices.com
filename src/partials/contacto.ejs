<section class="container">
  <div class="mb-3">
    <label for="FormContactoEmail" class="form-label">Email</label>
    <input
      type="email"
      class="form-control"
      id="FormContactoEmail"
      placeholder="nombre@dominio.com"
    />
  </div>
  <div class="mb-3">
    <label for="FormContactoTexto" class="form-label">Mensaje</label>
    <textarea id="FormContactoTexto" rows="5" class="form-control"></textarea>
  </div>
  <button class="btn btn-primary" type="submit">Enviar</button>
</section>
